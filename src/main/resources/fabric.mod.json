  "authors": [
    "IMB11 (mineblock11)"
  ],
  "contact": {
    "website": "https://imb11.dev/",
    "repo": "https://github.com/IMB11/Sounds"
  },
  "license": "ARR",
  "icon": "assets/sounds/icon.png",
